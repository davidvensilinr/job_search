generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model jobs {
  id                  Int     @id @default(autoincrement())
  company_name        String  @db.VarChar(255)
  job_role            String  @db.VarChar(255)
  company_description String  @db.VarChar(500)
  job_description     String  @db.VarChar(500)
  lpa                 Decimal @db.Decimal(10, 2)
}
